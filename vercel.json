{
    "version": 2,
    "name": "prompay-backend",
    "builds": [
       { "src": "index.js", "use": "@vercel/node" }
    ],
    "routes": [
       { "src": "/(.*)", "dest": "/index.js" }
    ],
    "functions": {
      "controllers/*.js": {
        "maxDuration": 60
      },
      "middleware/**/*.js": {
        "maxDuration": 60
      }
    }
 }